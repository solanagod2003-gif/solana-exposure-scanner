{
    "version": 2,
    "buildCommand": "npm run vercel-build",
    "outputDirectory": "dist",
    "functions": {
        "api/**/*.ts": {
            "runtime": "@vercel/node@3"
        }
    },
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "/api"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}